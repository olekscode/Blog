library(ggplot2)
library(ggthemes)
library(grid)
library(gridExtra)
library(scales)
areas <- runif(50) * 40 + 10
prices <- areas * 10 + 200 + rnorm(length(areas)) * 70
data <- data.frame(
area = areas,
price = prices)
ggplot(data, aes(x=area, y=price)) +
geom_point() +
geom_smooth(method='lm', se = FALSE, color=red) +
ggtitle('Apartment prices') +
labs(x="Area (sq. m)", y="Price (euros)") +
theme_Publication() +
theme(panel.grid.major=element_blank())
ggsave('goodRegressionLine.png', dpi=800)
ggsave('../img/goodRegressionLine.png', dpi=800)
ggplot(data, aes(x=area, y=price)) +
geom_point() +
geom_smooth(method='lm', se = FALSE, color=blue) +
ggtitle('Apartment prices') +
labs(x="Area (sq. m)", y="Price (euros)") +
theme_Publication() +
theme(panel.grid.major=element_blank())
ggplot(data, aes(x=area, y=price)) +
geom_point() +
geom_smooth(method='lm', se = FALSE, color=green) +
ggtitle('Apartment prices') +
labs(x="Area (sq. m)", y="Price (euros)") +
theme_Publication() +
theme(panel.grid.major=element_blank())
ggplot(data, aes(x=area, y=price)) +
geom_point() +
geom_smooth(method='lm', se = FALSE, color=yellow) +
ggtitle('Apartment prices') +
labs(x="Area (sq. m)", y="Price (euros)") +
theme_Publication() +
theme(panel.grid.major=element_blank())
ggsave('../img/goodRegressionLine.png', dpi=800)
